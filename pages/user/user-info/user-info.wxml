<import src="../../../templates/toast/toast.wxml" />
<scroll-view scroll-y="trues" class="user">
  <view class="list" hover-class="none">
      <view class="list-text">头像</view>
      <view class="list-info avatar"><open-data type="userAvatarUrl"></open-data></view>
      <view class="arrow-wrap"><image src="../images/accessory.png" class="list-arrow"></image></view>
  </view>
  <view class="list" hover-class="none">
        <view class="list-text">昵称</view>
        <view class="list-info"><open-data type="userNickName"></open-data></view>
        <view class="arrow-wrap">{{tip}}<image src="../images/accessory.png" class="list-arrow"></image></view>
  </view>
</scroll-view>
 <template is="toast" data="{{...toast}}" />